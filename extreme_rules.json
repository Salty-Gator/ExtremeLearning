  "patterns": {
    "mac_address": {
      "regex": "\\b([0-9A-Fa-f]{2}[:-]){5}[0-9A-Fa-f]{2}\\b",
      "description": "MAC address"
    },
    "vlan_id": {
      "regex": "\\bVLAN\\s*(\\d{1,4})\\b",
      "description": "VLAN ID (0-4094)"
    },
    "ipv4": {
      "regex": "\\b(?:(?:25[0-5]|2[0-4]\\d|1?\\d?\\d)\\.){3}(?:25[0-5]|2[0-4]\\d|1?\\d?\\d)\\b",
      "description": "IPv4 address"
    },
    "ipv6": {
      "regex": "\\b([A-Fa-f0-9]{1,4}:){7}[A-Fa-f0-9]{1,4}\\b",
      "description": "IPv6 (simple match)"
    },
    "gtac_case": {
      "regex": "\\b0\\d{7}\\b",
      "description": "GTAC case number (8 digits starting with 0)"
    },
    "serial_number": {
      "regex": "\\b([A-Z0-9]{8,14})\\b",
      "description": "Generic device serial"
    },
    "sku_switch": {
      "regex": "\\b(53|54|55|57|75)20-[0-9A-Z]+\\b",
      "description": "Switch SKU pattern"
    },
    "ap_sku": {
      "regex": "\\bAP[34]\\d{2}[A-Z]*\\b",
      "description": "AP3xx/AP4xx family"
    },
    "firmware_version_exos": {
      "regex": "\\b(3[01]|32)\\.\\d+(?:\\.\\d+)?\\b",
      "description": "EXOS firmware version"
    },
    "firmware_version_voss": {
      "regex": "\\b[0-9]{2}\\.\\d+(?:\\.\\d+)?\\b",
      "description": "VOSS firmware version"
    },
    "contract_po": {
      "regex": "\\b(PO|PR|CNTR)[-_ ]?[0-9A-Z]{6,}\\b",
      "description": "Purchase/contract identifiers"
    }
  },

  "negatives": [
    "fabric (textile|cotton|weave|cloth|garment)",
    "\\bEP1\\b (movie|episode|season)",
    "\\bVOSS\\b (water|bottle)",
    "spb.*(weave|loom)"
  ],

  "retrieval": {
    "metadata_boosts": {
      "product": {
        "Fabric Connect": 1.2,
        "Switch Engine (EXOS)": 1.1,
        "Fabric Engine (VOSS)": 1.1,
        "ExtremeCloud IQ Site Engine": 1.1
      },
      "doc_type": {
        "Release Notes": 1.1,
        "Configuration Guide": 1.2,
        "Best Practice": 1.3
      },
      "pub_recency_months": {
        "<=12": 1.2,
        "13-36": 1.05,
        ">36": 0.95
      }
    },
    "default_filters": {
      "when_query_contains": [
        {
          "contains": ["firmware", "upgrade", "release notes", "known issues"],
          "set_filter": {
            "doc_type_in": ["Release Notes", "Configuration Guide"]
          }
        },
        {
          "contains": ["multicast", "igmp", "pim", "spb"],
          "set_filter": {
            "product_in": ["Fabric Connect", "Fabric Engine (VOSS)", "Switch Engine (EXOS)"]
          }
        }
      ]
    },
    "top_k": {
      "semantic": 60,
      "keyword": 30,
      "rerank_select": 15
    }
  },

  "tagging": {
    "product_infer": [
      { "pattern_any": ["\\bEXOS\\b", "Switch Engine"], "value": "Switch Engine (EXOS)" },
      { "pattern_any": ["\\bVOSS\\b", "Fabric Engine"], "value": "Fabric Engine (VOSS)" },
      { "pattern_any": ["ExtremeCloud IQ Site Engine", "\\bXIQ[- ]?SE\\b", "\\bSite Engine\\b"], "value": "ExtremeCloud IQ Site Engine" },
      { "pattern_any": ["Fabric Connect", "\\bSPB\\b", "802\\.1aq"], "value": "Fabric Connect" },
      { "pattern_any": ["Extreme Platform ONE", "\\bEP1\\b"], "value": "Extreme Platform ONE" }
    ],
    "doc_type_infer": [
      { "pattern_any": ["Release Notes", "\\bKnown Issues\\b", "\\bResolved Issues\\b"], "value": "Release Notes" },
      { "pattern_any": ["User Guide", "Configuration Guide", "Install Guide", "CLI Reference"], "value": "Configuration Guide" },
      { "pattern_any": ["Quick Reference", "\\bQRG\\b"], "value": "Quick Reference Guide" },
      { "pattern_any": ["Best Practice", "Design Guide", "Deployment Guide"], "value": "Best Practice" }
    ],
    "extract_fields": {
      "firmware_exos": { "use_regex": "firmware_version_exos" },
      "firmware_voss": { "use_regex": "firmware_version_voss" },
      "vlan_ids": { "use_regex": "vlan_id" },
      "macs": { "use_regex": "mac_address" },
      "ips": { "use_regex": "ipv4" },
      "serials": { "use_regex": "serial_number" },
      "skus_switch": { "use_regex": "sku_switch" },
      "skus_ap": { "use_regex": "ap_sku" }
    }
  },

  "glossary": {
    "Fabric Connect": "Extreme’s SPB-based fabric technology that simplifies L2/L3 services and multicast.",
    "Switch Engine (EXOS)": "Extreme’s NOS for EXOS-based switches (e.g., 5320/5420/5520/5720).",
    "Fabric Engine (VOSS)": "Extreme’s NOS for VOSS-based systems, often used in Fabric Connect cores/edges.",
    "EP1": "Extreme Platform ONE—AI, automation, and unified workflows across Extreme solutions.",
    "E-rate IC": "Category 2 ‘Internal Connections’ (eligible hardware/software required to make it work).",
    "BMIC": "Basic Maintenance of Internal Connections.",
    "MIBS": "Managed Internal Broadband Services.",
    "ExtremeCloud IQ Site Engine": "Legacy XMC/NetSight rebranded as Site Engine. Provides on-prem management, inventory, configuration, compliance, topology, and integration with XIQ.",
    "ExtremeControl (NAC)": "Extreme’s NAC solution. Provides authentication, policy enforcement, device profiling, and guest/BYOD onboarding. Formerly NetSight NAC.",
    "ExtremeAnalytics": "Extreme’s application visibility and analytics platform. Originally Purview, provides layer 7 application identification, usage insights, and integration with NAC for policy."
  },

  "redaction": [
    { "regex": "\\b[0-9]{10,16}\\b", "replace": "[REDACTED_NUMBER]" },
    { "regex": "\\b[A-Z0-9._%+-]+@[A-Z0-9.-]+\\.[A-Z]{2,}\\b", "flags": "i", "replace": "[REDACTED_EMAIL]" },
    { "use_pattern": "gtac_case", "replace": "[GTAC_CASE]" },
    { "use_pattern": "serial_number", "replace": "[SERIAL]" },
    { "use_pattern": "contract_po", "replace": "[CONTRACT_ID]" }
  ],

  "query_expansion": {
    "expand_if_absent": [
      { "entity": "fabric_connect" },
      { "entity": "extremecloud_iq_site_engine" },
      { "entity": "extreme_switch_engine" },
      { "entity": "extreme_fabric_engine" },
      { "entity": "extreme_platform_one" }
    ],
    "negative_guards": "*use negatives above*",
    "max_added_terms": 8
  },

  "eval": {
    "must_cite": true,
    "prefer_recent_months": 36,
    "firmware_conflict_note": "If multiple firmware versions differ, state version-specific results.",
    "hard_facts_require_table": true
  }
}
