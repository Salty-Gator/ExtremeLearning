  "normalization": {
    "products": {
      "EXOS": "Switch Engine (EXOS)",
      "ExtremeXOS": "Switch Engine (EXOS)",
      "VOSS": "Fabric Engine (VOSS)",
      "XIQ-SE": "ExtremeCloud IQ Site Engine",
      "SiteEngine": "ExtremeCloud IQ Site Engine",
      "Extreme Cloud IQ": "ExtremeCloud IQ",
      "EP1": "Extreme Platform ONE",
      "ESE": "ExtremeCloud IQ Site Engine",
      "XMC": "ExtremeCloud IQ Site Engine",
      "NetSight": "ExtremeCloud IQ Site Engine",
      "NAC": "ExtremeControl (NAC)",
      "NetSight NAC": "ExtremeControl (NAC)",
      "Extreme NAC": "ExtremeControl (NAC)",
      "Purview": "ExtremeAnalytics",
      "XMC Analytics": "ExtremeAnalytics",
